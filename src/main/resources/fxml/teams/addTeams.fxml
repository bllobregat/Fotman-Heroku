<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="426.0" prefWidth="279.0"
            xmlns="http://javafx.com/javafx/11.0.1"
            fx:controller="com.example.version.controller.javafx.teams.AddTeamsController">
    <children>
        <TextField fx:id="txtName" layoutX="60.0" layoutY="104.0"/>
        <Label layoutX="60.0" layoutY="87.0" text="Name"/>
        <TextField fx:id="txtDivision" layoutX="60.0" layoutY="176.0"/>
        <Label layoutX="60.0" layoutY="159.0" text="Division"/>
        <TextField fx:id="txtStadium" layoutX="60.0" layoutY="248.0"/>
        <Label layoutX="60.0" layoutY="231.0" text="Stadium"/>
        <Button fx:id="btnButton" layoutX="60.0" layoutY="320.0" mnemonicParsing="false" onAction="#addTeams"
                prefHeight="32.0" prefWidth="149.0" text="Confirm"/>
        <Label alignment="CENTER" layoutX="57.0" layoutY="33.0" prefHeight="39.0" prefWidth="156.0" text="Add Team">
            <font>
                <Font name="System Bold" size="27.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
