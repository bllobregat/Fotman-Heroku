        </VBox>
        <Label layoutX="98.0" layoutY="390.0" prefHeight="25.0" prefWidth="177.0" text="Type"/>
        <TextField fx:id="txtType" layoutX="98.0" layoutY="415.0" prefHeight="25.0" prefWidth="177.0"/>

        <Label layoutX="364.0" layoutY="123.0" prefHeight="25.0" prefWidth="177.0" text="Personal Notes"/>
        <TextArea fx:id="txtPersonalNotes" layoutX="364.0" layoutY="161.0" prefHeight="279.0" prefWidth="274.0"/>

        <Button layoutX="406.0" layoutY="530.0" mnemonicParsing="false" onAction="#confirm" prefHeight="25.0"
                prefWidth="190.0" text="Confirm"/>
    </children>
</AnchorPane>
