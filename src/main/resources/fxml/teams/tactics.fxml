        </Label>
        <Label fx:id="lbName" contentDisplay="CENTER" layoutX="324.0" layoutY="42.0" prefHeight="46.0" prefWidth="274.0"
               text="Name">
            <font>
                <Font name="Elephant" size="24.0"/>
            </font>
        </Label>

        <Label layoutX="104.0" layoutY="123.0" prefHeight="25.0" prefWidth="177.0" text="Formation"/>
        <VBox layoutX="98.0" layoutY="182.0" prefHeight="177.0" prefWidth="164.0">
            <children>
                <RadioButton fx:id="rB442" mnemonicParsing="false" prefHeight="25.0" prefWidth="155.0"
                             text="4 - 4 - 2"/>
                <RadioButton fx:id="rB433" mnemonicParsing="false" prefHeight="25.0" prefWidth="155.0"
                             text="4 - 3 - 3"/>
                <RadioButton fx:id="rB541" mnemonicParsing="false" prefHeight="25.0" prefWidth="155.0"
                             text="5 - 4 - 1"/>
                <RadioButton fx:id="rB4321" mnemonicParsing="false" prefHeight="25.0" prefWidth="155.0"
                             text="4 - 2 - 3 - 1"/>
                <RadioButton fx:id="rB4141" layoutX="10.0" layoutY="85.0" mnemonicParsing="false" prefHeight="25.0"
