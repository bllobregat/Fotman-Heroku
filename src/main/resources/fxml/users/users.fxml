    <children>
        <TableView fx:id="tblUsers" layoutX="76.0" layoutY="83.0" prefHeight="541.0" prefWidth="480.0">
            <columns>
                <TableColumn fx:id="colName" prefWidth="144.0" text="Name" />
                <TableColumn fx:id="colSurname" prefWidth="234.0" text="Surname" />
                <TableColumn fx:id="colRole" prefWidth="101.0" text="Role" />
            </columns>
        </TableView>
        <Button layoutX="614.0" layoutY="83.0" mnemonicParsing="false" onAction="#addUsers" prefHeight="30.0" prefWidth="169.0" text="Add/Edit Users" />
        <Button layoutX="614.0" layoutY="135.0" mnemonicParsing="false" onAction="#removeUsers" prefHeight="30.0" prefWidth="169.0" text="Remove User" />
        <Button layoutX="614.0" layoutY="189.0" mnemonicParsing="false" onAction="#showContactDetails" prefHeight="30.0" prefWidth="169.0" text="Contact Details" />
        <Button layoutX="614.0" layoutY="594.0" mnemonicParsing="false" onAction="#back" prefHeight="30.0" prefWidth="169.0" text="Next" />
        <Button layoutX="614.0" layoutY="544.0" mnemonicParsing="false" onAction="#printUsers" prefHeight="30.0" prefWidth="169.0" text="Print Users List" />
        <Label layoutX="68.0" layoutY="26.0" prefHeight="36.0" prefWidth="594.0" text="Users">
            <font>
                <Font name="Elephant" size="30.0" />
            </font>
        </Label>
        <AmbientLight color="CHARTREUSE" layoutX="322.0" layoutY="52.0" lightOn="true" />
    </children>

</AnchorPane>
